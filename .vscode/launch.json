{
    "configurations": [
    {
        "name": "(gdb) Attach",
        "type": "lldb",
        "request": "attach",
        "pid": "21582",
        "program": "/root/libdft/libdft64/tools/obj-intel64/track.so",
        "preLaunchTask": "preBuild",
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }
    ]
}



